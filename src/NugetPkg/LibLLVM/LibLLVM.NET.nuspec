<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata minClientVersion="2.8">
        <id>LibLLVM.NET</id>
        <version>$version$</version>
        <authors>LLVM.NET</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Extended LLVM-C API DLLs for LLVM v$llvmversion$</description>
        <developmentDependency>true</developmentDependency>
        <projectUrl>http://github.com/netmf/LLVM.NET</projectUrl>
        <licenseUrl>http://releases.llvm.org/$llvmversion$/LICENSE.TXT</licenseUrl>
        <tags>LLVM,compiler,native</tags>
    </metadata>
    <files>
        <file src="build\LibLLVM.NET.props" target="build\native\LibLLVM.NET.props" />
        <file src="$srcroot$\*.h" target="lib\native\include" exclude="resource.h;targetver.h;stdafx.h" />
        <file src="$buildcontentoutput$x86\$configuration$\LibLLVM.lib" target="build\LibLLVM\x86" />
        <file src="$buildcontentoutput$x86\$configuration$\LibLLVM.pdb" target="build\LibLLVM\x86" />
        <file src="$buildcontentoutput$x64\$configuration$\LibLLVM.lib" target="build\LibLLVM\x64" />
        <file src="$buildcontentoutput$x64\$configuration$\LibLLVM.pdb" target="build\LibLLVM\x64" />

        <file src="$buildbinoutput$x86\$configuration$\LibLLVM.dll" target="build\LibLLVM\x86" />
        <file src="$buildbinoutput$x64\$configuration$\LibLLVM.dll" target="build\LibLLVM\x64" />
    </files>
</package>
