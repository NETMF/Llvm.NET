<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata minClientVersion="2.8">
        <id>Llvm.Libs</id>
        <version>$version$</version>
        <authors>smaillet</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Pre-built LLVM libraries and headers for LLVM $version$</description>
        <developmentDependency>true</developmentDependency>
        <projectUrl>http://www.llvm.org</projectUrl>
        <licenseUrl>http://releases.llvm.org/$version$/LICENSE.TXT</licenseUrl>
        <tags>LLVM,compiler,native</tags>
        <dependencies>
            <dependency id="Llvm.Libs.core.x86-Release" version="[$version$]"/>
            <dependency id="Llvm.Libs.core.x86-Debug" version="[$version$]"/>
            <dependency id="Llvm.Libs.core.x64-Release" version="[$version$]"/>
            <dependency id="Llvm.Libs.core.x64-Debug" version="[$version$]"/>

            <dependency id="Llvm.Libs.core.pdbs.x86-Debug" version="[$version$]"/>
            <dependency id="Llvm.Libs.core.pdbs.x64-Debug" version="[$version$]"/>

            <dependency id="Llvm.Libs.targets.x86-Release" version="[$version$]"/>
            <dependency id="Llvm.Libs.targets.x86-Debug" version="[$version$]"/>
            <dependency id="Llvm.Libs.targets.x64-Release" version="[$version$]"/>
            <dependency id="Llvm.Libs.targets.x64-Debug" version="[$version$]"/>
        </dependencies>
    </metadata>
    <files>
        <file src="Llvm.Libs.MetaPackage.props" target="build\native\Llvm.Libs.props" />
        <file src="$llvmsrcroot$\include\**" target="lib\native\include" />
    </files>
</package>
