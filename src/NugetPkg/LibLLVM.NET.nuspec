<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata minClientVersion="2.8">
        <id>LibLLVM.NET</id>
        <version>$version$</version>
        <authors>LLVM.NET</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Extended LLVM-C API DLLs for LLVM $version$</description>
        <developmentDependency>true</developmentDependency>
        <projectUrl>http://github.com/netmf/LLVM.NET</projectUrl>
        <licenseUrl>http://releases.llvm.org/$version$/LICENSE.TXT</licenseUrl>
        <tags>LLVM,compiler,native</tags>
    </metadata>
    <files>
        <file src="LibLLVM.NET.props" target="build\native\LibLLVM.NET.props" />
        <file src="$srcroot$\include\**" target="lib\native\include" exclude="resource.h;targetver.h;stdafx.h" />
        <file src="$buildoutput$\$version$\x86-Debug\Debug\lib\*"      target="lib\native\x86-Debug\lib" />
    </files>
</package>
